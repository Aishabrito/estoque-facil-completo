generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Produto {
  id        Int      @id @default(autoincrement())
  nome      String
  categoria String?
  preco     Decimal  @db.Decimal(10, 2)
  estoque   Int      @default(0)
  criadoEm  DateTime @default(now()) @map("criado_em")

  @@map("produtos")
}